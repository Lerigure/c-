#include<iostream>
using namespace std;
int main(){
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int start=a*60+b;
    int end=c*60+d;
    int spent_time=end-start;
    if(spent_time<=0)spent_time+=1440;
    cout<<"O JOGO DUROU "<<spent_time/60<<" HORA(S) E "<<spent_time%60<<" MINUTO(S)"<<endl;
    return 0;
}
